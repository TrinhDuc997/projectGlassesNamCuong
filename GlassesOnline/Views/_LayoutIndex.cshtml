                    <div class="box-content box-category">
                        <form action="/Index/ViewTheoDieuKien" method="post" >
                            <ul id="custom_accordion">

                                <li class="radio">
                                    <label><input type="radio" name="GioiTinh" value="true" checked>Nam</label>
                                </li>
                                <li class="radio">
                                    <label><input type="radio" name="GioiTinh" value="false">Nữ</label>
                                </li>
                            </ul>
                            <select id="KhoangGia" name="KhoangGia">
                                <option value="0">Khoảng Giá</option>
                                <option value="200">Dưới 200.000</option>
                                <option value="500">từ 200.000 đến 500.000</option>
                                <option value="1000">từ 500.000 đến 1.000.000</option>
                                <option value="1100">Trên 1.000.000</option>
                            </select>
                            <select id="KieuGiang" name="KieuDang">
                                <option value="All">Kiểu Dáng</option>
                                <option value="Tròn">Tròn</option>
                                <option value="Vuông">Vuông</option>
                                <option value="Oval">Oval</option>
                                <option value="Rimless">Rimless</option>
                            </select>
                            <select id="ThuongHieu" name="ThuongHieu">
                                <option value="All">Thuong Hieu</option>
                                <option value="Gucci">Gucci</option>
                                <option value="Prada">Prada</option>
                                <option value="Coach">Coach</option>
                                <option value="Puma">Puma</option>
                            </select>
                            <button id="btnLocSP" type="submit" value="">Lọc Sản Phẩm</button>
                        </form>
                    </div>
                </div>
                <!--Categories Part End-->
                <!--Latest Product Start-->
                <div class="box">
                    <div class="box-heading">Sản Phẩm Mua Nhiều</div>
                    <div class="box-content">
                        @Html.Action("PartialBuyMore", "PartialView");
                    </div>
                </div>
            </div>
            <!--Left End-->
            <!--Middle Part Start-->
            @RenderBody()
