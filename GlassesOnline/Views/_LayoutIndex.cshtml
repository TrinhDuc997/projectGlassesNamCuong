            <div id="welcome">
                <div id="language">
                    Ngôn Ngữ
                    <ul>
                        <li><a title="English"><img src="~/Content/Images/Design/flags/gb.png" alt="English" />English</a></li>
                    </ul>
                </div>
                <div class="links">
                    Tài Khoản
                    <ul>
                        <li><a href="#">Tài Khoản Của Bạn</a></li>
                        <li><a href="~/GioHang/ViewGioHang">Giỏ Hàng Của Bạn</a></li>
                    </ul>
                </div>
                @{
                    if (Session["MaKH"] == null)
                    {
                        <a href="~/DKDN/ViewDangNhap">Đăng Nhập</a>
                    }

                    else
                    {
                        <a id="TenKhachHang" href="#">Xin Chào! @Session["TenKH"]</a>
                    }

                }
                
                <a href="~/DKDN/ViewDangKy">Tạo Tài Khoản</a>
